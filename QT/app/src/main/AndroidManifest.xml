<?xml version="1.1" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="quadroteam.qt">

    <application
        android:allowBackup="true"
        android:configChanges="orientation|screenSize"
        android:icon="@mipmap/logo"
        android:label="Дзета"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LevelActivity" />
        <activity android:name=".TheoryActivity" />
        <activity android:name=".TaskActivity" />
        <activity android:name=".LibActivity" />
        <activity android:name=".LibOfTheory" />
        <activity android:name=".DevelopersActivity" />
        <activity
            android:name=".Congratulations"
            android:label="@string/title_activity_congratulations"
            android:theme="@style/Theme.AppCompat.NoActionBar"></activity>
    </application>

</manifest>
